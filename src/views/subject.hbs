<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Curriculum - IT Resource Bank</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicon -->
    <link href="img/favicon.ico" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link
        href="https://fonts.googleapis.com/css2?family=Heebo:wght@400;500;600&family=Nunito:wght@600;700;800&display=swap"
        rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="lib/animate/animate.min.css" rel="stylesheet">
    <link href="lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">

    <!-- Customized Bootstrap Stylesheet -->
    <link href="css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="css/style.css" rel="stylesheet">
</head>

<body>
    {{>header}}

    <div class="modal fade" id="showFileModal" tabindex="-1" aria-labelledby="showFileModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="showFileModalLabel">Details</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" data-bs-dismiss="modal"
                        data-bs-target="#showFileModal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <div class="card mb-3">
                        <div class="row g-0">
                            <div class="col-md-2">
                                <img class="img-fluid rounded-start" id="subjectThumbnail"
                                    src="img/preview_thumbnail.png" alt="Thumbnail">
                            </div>
                            <div class="col-md-10">
                                <div class="card-body">
                                    <h5 class="card-title" id="subjectTitle">Card title</h5>
                                    <p class="card-text" id="subjectUpvotes"><b>Close and Reopen...</b> </p>
                                    <p class="card-text" id="subjectDownvotes"><b>Close and Reopen...</b> </p>
                                    <p class="card-text" id="subjectAuther"><b>Close and Reopen...</b> </p>
                                    <p class="card-text" id="subjectTopics"><b>Topics Covered:</b> </p>
                                    <p class="card-text" id="subjectDesc"><b>Description:</b> </p>
                                    <input hidden type="text" id="subjectID">
                                    <input hidden type="text" id="subjectFullName">
                                </div>
                            </div>
                        </div>
                    </div>
                    <p class="font-monospace fs-6 text-end" style="color: dodgerblue;" id="download_message"></p>
                </div>
                <div class="modal-footer" id="modal-buttons-subject">
                    <button type="button" id="openFileBtn" class="btn btn-primary">Open File</button>
                    <button type="button" id="downloadFileBtn" class="btn btn-primary">Download File</button>
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal"
                        data-bs-target="#showFileModal" aria-label="Close" id="showFileModalClose">Close</button>
                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="showURLModal" tabindex="-1" aria-labelledby="showURLModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="showURLModalLabel">Details</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" data-bs-dismiss="modal"
                        data-bs-target="#showURLModal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <div class="card mb-3">
                        <div class="row g-0">
                            <div class="col-md-12">
                                <div class="card-body">
                                    <h5 class="card-title" id="urlTitle">Card title</h5>
                                    <p class="card-text" id="urlUpvotes"><b>Close and Reopen...</b> </p>
                                    <p class="card-text" id="urlDownvotes"><b>Close and Reopen...</b> </p>
                                    <p class="card-text" id="urlAuther"><b>Close and Reopen...</b> </p>
                                    <p class="card-text" id="urlTopics"><b>Topics Covered:</b> </p>
                                    <p class="card-text" id="urlDesc"><b>Description:</b> </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer" id="modal-buttons-url">
                    <a id="visitURLBtn" target="_blank" class="btn btn-primary">Visit</a>
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal"
                        data-bs-target="#showURLModal" aria-label="Close" id="showURLModalClose">Close</button>
                </div>
            </div>
        </div>
    </div>

    <!-- Header Start -->
    <div class="container-fluid bg-primary py-5 mb-5 page-header">
        <div class="container py-5">
            <div class="row justify-content-center">
                <div class="col-lg-10 text-center">
                    <h1 id="subject_name" class="display-3 text-white animated slideInDown"></h1>
                </div>
            </div>
        </div>
    </div>
    <!-- Header End -->


    <form id="subjectForm" hidden>
        <input id="subName" value="{{subName}}" hidden>
        <input id="subID" value="{{subID}}" hidden>
    </form>

    <!-- Service Start -->
    <div class="container-xxl py-5">
        <div class="container">
            <div class="text-center wow fadeInUp" data-wow-delay="0.1s">
                <h6 class="section-title bg-white text-center text-primary px-3">Notes</h6>
                <h1 class="mb-5">Notes By Students</h1>
            </div>
            <div class="row g-4" id="Notes">
            </div>
        </div>
        <br>
        <div class="container">
            <div class="text-center wow fadeInUp" data-wow-delay="0.1s">
                <h6 class="section-title bg-white text-center text-primary px-3">PPTs</h6>
                <h1 class="mb-5">Power Point Presentation</h1>
            </div>
            <div class="row g-4" id="PPT">
            </div>
        </div>
        <br>
        <div class="container">
            <div class="text-center wow fadeInUp" data-wow-delay="0.1s">
                <h6 class="section-title bg-white text-center text-primary px-3">Books</h6>
                <h1 class="mb-5">Digital Books</h1>
            </div>
            <div class="row g-4" id="BOOKS">
            </div>
        </div>
        <br>
        <div class="container">
            <div class="text-center wow fadeInUp" data-wow-delay="0.1s">
                <h6 class="section-title bg-white text-center text-primary px-3">YouTube</h6>
                <h1 class="mb-5">YouTube Playlists</h1>
            </div>
            <div class="row g-4" id="YTPlaylist">
            </div>
        </div>
        <br>

        <div id="Others" class="container">
            <div class="text-center wow fadeInUp" data-wow-delay="0.1s">
                <h6 class="section-title bg-white text-center text-primary px-3">Others</h6>
                <h1 class="mb-5">Other Resources</h1>
            </div>
        </div>
    </div>
    <br>
    </div>
    <!-- Service End -->

    {{>footer}}

    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="lib/wow/wow.min.js"></script>
    <script src="lib/easing/easing.min.js"></script>
    <script src="lib/waypoints/waypoints.min.js"></script>
    <script src="lib/owlcarousel/owl.carousel.min.js"></script>

    <!-- Template Javascript -->
    <script src="js/main.js"></script>
    <script src="js/subject.js"></script>
    <script>
        window.onload = function () {
            document.getElementById("home_menu").classList.remove("active");
            document.getElementById("curriculam_dropdown").classList.add("active");
            document.getElementById("placements_dropdown").classList.remove("active");
            document.getElementById("feedback_menu").classList.remove("active");
            document.getElementById("me_dropdown").classList.remove("active");
        }
    </script>
</body>

</html>