<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Curriculum - IT Resource Bank</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicon -->
    <link href="img/favicon.ico" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Heebo:wght@400;500;600&family=Nunito:wght@600;700;800&display=swap" rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="lib/animate/animate.min.css" rel="stylesheet">
    <link href="lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">

    <!-- Customized Bootstrap Stylesheet -->
    <link href="css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="css/style.css" rel="stylesheet">
</head>

<body>
    {{>header}}

    <!-- Header Start -->
    <div class="container-fluid bg-primary py-5 mb-5 page-header">
        <div class="container py-5">
            <div class="row justify-content-center">
                <div class="col-lg-10 text-center">
                    <h1 id="subject_name" class="display-3 text-white animated slideInDown"></h1>
                </div>
            </div>
        </div>
    </div>
    <!-- Header End -->

    {{>preview}}

    <form id="subjectForm" hidden>
        <input id="subName" value="{{subName}}" hidden>
        <input id="bookFiles" value="{{bookF}}" hidden>
        <input id="notesFiles" value="{{notesF}}" hidden>
        <input id="pptFiles" value="{{pptF}}" hidden>
        <input id="otherFiles" value="{{otherF}}" hidden>
        <input id="excelFiles" value="{{excelF}}" hidden>
    </form>

    <!-- Service Start -->
    <div class="container-xxl py-5">
        <div class="container">
            <div class="text-center wow fadeInUp" data-wow-delay="0.1s">
                <h6 class="section-title bg-white text-center text-primary px-3">Notes</h6>
                <h1 class="mb-5">Notes By Students</h1>
            </div>
            <div class="row g-4" id="notes">
            </div>
        </div>
        <br>
        <div class="container">
            <div class="text-center wow fadeInUp" data-wow-delay="0.1s">
                <h6 class="section-title bg-white text-center text-primary px-3">PPTs</h6>
                <h1 class="mb-5">Power Point Presentation</h1>
            </div>
            <div class="row g-4" id="ppts">
            </div>
        </div>
        <br>
        <div class="container">
            <div class="text-center wow fadeInUp" data-wow-delay="0.1s">
                <h6 class="section-title bg-white text-center text-primary px-3">Books</h6>
                <h1 class="mb-5">Digital Books</h1>
            </div>
            <div class="row g-4" id="books">
            </div>
        </div>
        <br>
        <div class="container">
            <div class="text-center wow fadeInUp" data-wow-delay="0.1s">
                <h6 class="section-title bg-white text-center text-primary px-3">YouTube</h6>
                <h1 class="mb-5">YouTube Playlists</h1>
            </div>
            <div class="row g-4" id="ytplaylist">
            </div>
        </div>
        <br>
        <div class="container">
            <div class="text-center wow fadeInUp" data-wow-delay="0.1s">
                <h6 class="section-title bg-white text-center text-primary px-3">Others</h6>
                <h1 class="mb-5">Other Resources</h1>
            </div>
            <div class="row g-4" id="others">
            </div>
        </div>
        <br>
    </div>
    <!-- Service End -->

    {{>footer}}

    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="lib/wow/wow.min.js"></script>
    <script src="lib/easing/easing.min.js"></script>
    <script src="lib/waypoints/waypoints.min.js"></script>
    <script src="lib/owlcarousel/owl.carousel.min.js"></script>

    <!-- Template Javascript -->
    <script src="js/main.js"></script>
    <script src="js/subject.js"></script>
    <script>
        window.onload = function(){
            document.getElementById("home_menu").classList.remove("active");
            document.getElementById("curriculam_dropdown").classList.add("active");
            document.getElementById("placements_dropdown").classList.remove("active");
            document.getElementById("feedback_menu").classList.remove("active");
            document.getElementById("team_members").classList.remove("active");
        }
    </script>
</body>
</html>